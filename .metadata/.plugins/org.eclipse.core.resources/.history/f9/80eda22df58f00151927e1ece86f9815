package br.edu.fa7.e2.SS;

import java.util.HashMap;
import java.util.List;

public class Student {
	
	private String name;
	private String registry;
	private List<String> topMatchs;
	private HashMap<String, Double> grades;
	private HashMap<String, Double> profile;
	private List<String> warningSubjects;
	private List<String> recommendedSubjects;
	
	public Student(){
		
	}
	
	public Student(String name, String registry, List<String> topMatchs,
			HashMap<String, Double> grades, HashMap<String, Double> profile,
			List<String> warningSubjects, List<String> recommendedSubjects) {
		super();
		this.name = name;
		this.registry = registry;
		this.topMatchs = topMatchs;
		this.grades = grades;
		this.profile = profile;
		this.warningSubjects = warningSubjects;
		this.recommendedSubjects = recommendedSubjects;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getRegistry() {
		return registry;
	}
	public void setRegistry(String registry) {
		this.registry = registry;
	}
	public List<String> getTopMatchs() {
		return topMatchs;
	}
	public void setTopMatchs(List<String> topMatchs) {
		this.topMatchs = topMatchs;
	}
	public HashMap<String, Double> getGrades() {
		return grades;
	}
	public void setGrades(HashMap<String, Double> grades) {
		this.grades = grades;
	}
	public HashMap<String, Double> getProfile() {
		return profile;
	}
	public void setProfile(HashMap<String, Double> profile) {
		this.profile = profile;
	}
	public List<String> getWarningSubjects() {
		return warningSubjects;
	}
	public void setWarningSubjects(List<String> warningSubjects) {
		this.warningSubjects = warningSubjects;
	}
	public List<String> getRecommendedSubjects() {
		return recommendedSubjects;
	}
	public void setRecommendedSubjects(List<String> recommendedSubjects) {
		this.recommendedSubjects = recommendedSubjects;
	}
	
	
}
